SELECT 'Orçamentos criados:' as validacao, COUNT(*) as total FROM orcamento;
SELECT 'Itens orçados:' as validacao, COUNT(*) as total FROM item_orcamento;

-- Orçamentos por fornecedor
SELECT
    f.id,
    u.nome as fornecedor,
    COUNT(o.id) as total_orcamentos,
    SUM(CASE WHEN o.status = 'PENDENTE' THEN 1 ELSE 0 END) as pendentes,
    SUM(CASE WHEN o.status = 'ACEITO' THEN 1 ELSE 0 END) as aceitos,
    SUM(CASE WHEN o.status = 'REJEITADO' THEN 1 ELSE 0 END) as rejeitados
FROM fornecedor f
INNER JOIN usuario u ON f.id = u.id
LEFT JOIN orcamento o ON f.id = o.id_fornecedor_prestador
GROUP BY f.id, u.nome
ORDER BY total_orcamentos DESC;

-- Demandas por casal
SELECT
    c.id,
    u1.nome || ' & ' || u2.nome as casal,
    COUNT(d.id) as total_demandas
FROM casal c
INNER JOIN usuario u1 ON c.id_noivo1 = u1.id
LEFT JOIN usuario u2 ON c.id_noivo2 = u2.id
LEFT JOIN demanda d ON c.id = d.id_casal
GROUP BY c.id, u1.nome, u2.nome
ORDER BY c.id;
